{
  "mutableExtensionsDir": false,
  "name": "p-sharp-lang",
  "displayName": "P-Sharp Lang",
  "publisher": "stephanelatil",
  "description": "Provides language support for the P# (P-Sharp) language",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.2.0"
  },
  "engine": {
    "vscode": "^1.2.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "psc",
        "aliases": ["P#", "psc"],
        "extensions": [".psc"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "psc",
        "scopeName": "source.psharp",
        "path": "./syntaxes/psc.tmLanguage.json"
      }
    ]
  }
}